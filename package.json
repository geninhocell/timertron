{
  "name": "timertron",
  "productName": "Timer Tron",
  "version": "0.1.2",
  "author": "geninhocell@gmail.com",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "start": "electron .",
    "build:all": "electron-packager ./ timertron --platform=darwin,linux,win32 --arch=x64 --icon=icon/icon --out=dist/",
